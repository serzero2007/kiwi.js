{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "max-classes-per-file": 5,
      "prefer-const": false,
      "member-ordering": false,
      "variable-name": false,
      "semicolon": false,
      "trailing-comma": false,
      "curvy": false,
      "arrow-parens": false
    },
    "rulesDirectory": []
}
